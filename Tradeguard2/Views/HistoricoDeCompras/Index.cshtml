@model IEnumerable<Tradeguard2.Models.HistoricoDeCompra>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Anuncio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CC_vendedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CC_comprador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Preco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.vendido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Anuncio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CC_vendedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CC_comprador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Preco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.vendido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_historico">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_historico">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_historico">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
